<%
	var logo = theme.logo;
	if (!logo) {
		logo = theme.image_src;
	}
	logo = url_for(logo);
%>

					<ul class="right hide-on-med-and-down">
<%
	for (var i in theme.menu) {
%>
						<li ><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
<%
	} // end for
%>
					</ul>
					<div class="side-nav" id="mobile-demo">
						<div class="box-logo"><a href="<%- url_for(config.root) %>" title="<%= config.title %>"><img src="<%= logo %>" class="img-logo"></a></div>

						<ul>
<%
	for (var i in theme.menu) {
%>
						<li ><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
<%
	} // end for
%>
						</ul>
					</div>

<script type="text/javascript">
$(function(){
	$(".button-collapse").sideNav();
});
</script>
